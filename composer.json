{
  "name": "dravencms/structure",
  "type": "dravencms-package",
  "description": "Structure module for dravencms",
  "keywords": [
    "dravencms",
    "structure"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "salamek/nette-cms": "@dev",
    "dravencms/locale": "@dev",
    "dravencms/admin": "@dev"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "timeline": "Dravencms\\Structure\\DI\\StructureExtension"
        }
      },
      "scripts": {
        "post-install": "Dravencms\\Structure\\Script\\PostInstall",
        "pre-uninstall": "Dravencms\\Structure\\Script\\PreUninstall"
      }
    }
  }
}
